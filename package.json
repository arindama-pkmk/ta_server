{
  "name": "ta_server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "npm run start",
    "build": "tsc",
    "postinstall": "prisma generate --schema=src/prisma/schema.prisma",
    "lint": "eslint 'src/**/*.ts' --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prettier": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "format": "npm run prettier",
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "seeds": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "start:prod": "NODE_ENV=production node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "axios": "^1.8.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "inversify": "^6.2.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "pm2": "^5.4.3",
    "prisma": "^6.2.1",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ta_server": "file:",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.10.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
}
