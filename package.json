    "build": "tsc",
    "dev": "npm run start",
    "format": "npm run prettier",
    "generate": "prisma generate",
    "lint": "eslint 'src/**/*.ts' --fix",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev && prisma db seed",
    "migrate:reset": "prisma migrate reset",
